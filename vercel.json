{
  "buildCommand": "npm run build",
  "outputDirectory": "../dist",
  "framework": "vite",
  "routes": [
    { "src": "/under-development", "dest": "/index.html" },
    { "src": "/gif/(.*)", "dest": "/gif/$1" },
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
